    pcall(function()
        while wait() do
            if _G.AutoFarm or
                _G.Auto_Dungeon or
                _G.AutoFarmBoss or
                _G.AutoFarmBossHallow or
                _G.AutoFarmPrince or
                _G.SailBoat or
